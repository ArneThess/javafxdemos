<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+" codebase="http://javafxdemos.googlecode.com/hg/resource/DragNDrop/" href="DragAndDrop_browser.jnlp">
    <information>
        <title>DragAndDrop</title>
        <vendor>Rakesh Menon</vendor>
        <homepage href=""/>
        <description>DragAndDrop</description>
        <offline-allowed/>
    </information>
    <security>
        <all-permissions/>
    </security>
    <resources>
        <j2se version="1.5+"/>
        <property name="jnlp.packEnabled" value="true"/>
        <extension name="JavaFX Runtime" href="http://dl.javafx.com/1.2/javafx-rt.jnlp"/>
        <jar href="DragAndDrop.jar" main="true"/>
    </resources>
    <applet-desc name="DragAndDrop" main-class="com.sun.javafx.runtime.adapter.Applet" width="300" height="225">
        <param name="MainJavaFXScript" value="dragndrop.Main">
    </applet-desc>
    <update check="background">
</jnlp>
